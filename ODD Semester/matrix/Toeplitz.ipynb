    "# \n",
    "def solve( matrix):\n",
    "      for i in range(len(matrix)-1):\n",
    "         for j in range(len(matrix[0])-1):\n",
    "            if matrix[i][j]!=matrix[i+1][j+1]:\n",
    "               return False\n",
    "      return True\n",
    "\n",
    "matrix = [ [7, 2, 6], [3, 7, 2], [5, 3, 7]]\n",
    "print(solve(matrix))"
   ]
