   "source": [
    "# file in append mode\n",
    "f = open('sample.txt', 'a+')\n",
    "count_alp = 0\n",
    "f.seek(0)\n",
    "data = f.read()\n",
    "for i in data:\n",
    "    if i.isalpha():\n",
    "        count_alp += 1\n",
    "\n",
    "f.write(f'\\n\\nTotal Number of Alphabets {count_alp}')\n",
    "\n",
    "f.close()"
   ]
  }
 ],
